#:kivy 1.10.1

<Complete>:
    RelativeLayout:
        size_hint: None, None
        size: 213, 31
        center_x: self.parent.width / 2
        top: self.parent.height - 179

        Image:
            size_hint: None, None
            size: 31, 31
            source: 'assets/complete.png'
            pos_hint: {'center_y': .5}

        Label:
            size_hint: None, None
            text: 'Print Job Started'
            font_size: 22
            color: .21, .21, .25, 1
            size: self.texture_size
            right: 58
            pos_hint: {'center_y': .5}

    RelativeLayout:
        size_hint: None, None
        size: 450, 43
        center_x: self.parent.width / 2
        top: self.parent.height - 275

        ConfirmBtn:
            text: 'Continue Printing'
            on_press: root.manager.current = 'home'

        CancelBtn:
            text: 'Back to Home Page'
            right: self.parent.width
            on_press: root.manager.current = 'home'
